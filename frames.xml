<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">



   <Frame name="GreatVaultListTableCellTextTemplate" mixin="GreatVaultListTableCellTextMixin" virtual="true">
		<Size x="0" y="16"/>
		<Layers>
			<Layer level="BACKGROUND">
				<FontString parentKey="Text" inherits="Number14FontWhite" justifyH="LEFT" setAllPoints="true"/>
			</Layer>
		</Layers>
	</Frame>


   <Frame name="GreatVaultListTableCellTripleTextTemplate" mixin="GreatVaultListTableCellTripleTextMixin" virtual="true">
		<Size x="0" y="16"/>
      <Frames>
			<Frame parentKey="Text1" inherits="GreatVaultListTableCellTextTemplate">
				<Anchors>
					<Anchor point="LEFT"  x="0" y="0"/>
				</Anchors>
			</Frame>
         <Frame parentKey="Text2" inherits="GreatVaultListTableCellTextTemplate">
				<Anchors>
					<Anchor point="CENTER" x="0" y="0"/>
				</Anchors>
			</Frame>
         <Frame parentKey="Text3" inherits="GreatVaultListTableCellTextTemplate">
				<Anchors>
					<Anchor point="RIGHT" x="0" y="0"/>
				</Anchors>
			</Frame>
      </Frames>
	</Frame>




	<Button name="GreatVaultListTableHeaderStringTemplate" mixin="GreatVaultListTableHeaderStringMixin" inherits="ColumnDisplayButtonShortTemplate" virtual="true">
		<Size x="0" y="19"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Arrow" atlas="auctionhouse-ui-sortarrow" useAtlasSize="true">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.Text" relativePoint="RIGHT" x="3" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick method="OnClick"/>
		</Scripts>
	</Button>



   <Button name="GreatVaultListItemListLineTemplate" mixin="GreatVaultListItemListLineMixin" virtual="true">
		<Size x="0" y="20"/>
		<Anchors>
			<Anchor point="LEFT"/>
			<Anchor point="RIGHT"/>
		</Anchors>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="SelectedHighlight" atlas="auctionhouse-ui-row-select" alphaMode="ADD" hidden="true"/>

            <Texture parentKey="CurrentTexture" atlas="auctionhouse-ui-row-highlight" alphaMode="ADD" setAllPoints="true" hidden="true" alpha="0.8">
               <Color r="0.4" g="1" b="0.4" a="1" />
            </Texture>

				<!-- Using a normal texture for highlight to control the behavior. -->
				<Texture parentKey="HighlightTexture" atlas="auctionhouse-ui-row-highlight" alphaMode="ADD" setAllPoints="true" hidden="true" alpha="0.5" />
			</Layer>
		</Layers>
		<Scripts>
			<OnClick method="OnClick"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
		</Scripts>
		<NormalTexture parentKey="NormalTexture" atlas="auctionhouse-rowstripe-1" setAllPoints="true"/>
	</Button>







   <Frame name="GreatVaultListBackgroundTemplate" mixin="GreatVaultListBackgroundMixin" virtual="true">
		<KeyValues>
			<KeyValue key="layoutType" value="InsetFrameTemplate" type="string"/>

			<!--
			backgroundAtlas: atlas to be set in OnLoad.
			backgroundXOffset: X offset for the background image and nine slice.
			backgroundYOffset: Y offset for the background image and nine slice.

			<KeyValue key="backgroundAtlas" value="GreatVaultList-background-sell-left" type="string"/>
			<KeyValue key="backgroundXOffset" value="0" type="string"/>
			<KeyValue key="backgroundYOffset" value="0" type="string"/>
			-->
		</KeyValues>
		<Layers>
			<Layer>
				<Texture parentKey="Background">
					<Anchors>
						<Anchor point="TOPLEFT" x="3" y="-3"/>
                  <Anchor point="BOTTOMRIGHT" x="-20" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="NineSlice" useParentLevel="true" inherits="NineSlicePanelTemplate"/>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>




   <Frame name="GreatVaultListItemListHeadersTemplate" virtual="true">
		<Size x="0" y="19"/>
	</Frame>




   <Frame name="GreatVaultListItemListTemplate" mixin="GreatVaultListItemListMixin" inherits="GreatVaultListBackgroundTemplate" virtual="true">
		<KeyValues>
			<KeyValue key="layoutType" value="InsetFrameTemplate" type="string"/>
			<KeyValue key="backgroundYOffset" value="-19" type="number"/>
		</KeyValues>
		<Frames>

			<Frame parentKey="HeaderContainer" inherits="GreatVaultListItemListHeadersTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" x="4" y="-1"/>
					<Anchor point="TOPRIGHT" x="-26" y="-1"/>
				</Anchors>
			</Frame>
			<Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent.HeaderContainer" relativePoint="BOTTOMLEFT" x="0" y="-6"/>
					<Anchor point="RIGHT" relativeKey="$parent.HeaderContainer" relativePoint="RIGHT"/>
					<Anchor point="BOTTOM" x="0" y="3"/>
				</Anchors>
			</Frame>
			<EventFrame parentKey="ScrollBar" inherits="MinimalScrollBar">
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent.ScrollBox" relativePoint="TOPRIGHT" x="9" y="0"/>
					<Anchor point="BOTTOMLEFT" relativeKey="$parent.ScrollBox" relativePoint="BOTTOMRIGHT" x="9" y="4"/>
				</Anchors>
			</EventFrame>
			<Frame parentKey="LoadingSpinner" inherits="SpinnerTemplate" hidden="true">
				<Size x="90" y="90"/>
				<Anchors>
					<Anchor point="CENTER" x="-12" y="-15"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<FontString parentKey="SearchingText" inherits="GameFontNormalHuge4" text="SEARCHING">
							<Anchors>
								<Anchor point="BOTTOM" relativePoint="TOP" x="0" y="10"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		<Layers>
			<Layer level="OVERLAY">
				<FontString parentKey="ResultsText" inherits="GameFontNormal" hidden="true">
					<Anchors>
						<Anchor point="TOP" relativeKey="$parent.ScrollBox" relativePoint="TOP" x="0" y="-45"/>
						<Anchor point="LEFT" x="45"/>
						<Anchor point="RIGHT" x="-67"/>
					</Anchors>
					<Color color="NORMAL_FONT_COLOR"/>
					<Shadow>
						<Offset>
							<AbsDimension x="1" y="-1"/>
						</Offset>
						<Color r="0" g="0" b="0"/>
					</Shadow>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnUpdate method="OnUpdate"/>
		</Scripts>
	</Frame>


   <Frame name="GreatVaultListBrowseResultsFrameTemplate" mixin="GreatVaultListBrowseResultsFrameMixin" virtual="true">
		<Frames>
			<Frame parentKey="ItemList" inherits="GreatVaultListItemListTemplate">
				<KeyValues>
					<KeyValue key="hideStripes" value="true" type="boolean"/>
					<KeyValue key="backgroundAtlas" value="auctionhouse-background-index" type="string"/>
				</KeyValues>
				<Anchors>
					<Anchor point="TOPLEFT" y="-1"/>
					<Anchor point="BOTTOMRIGHT" x="18" y="0"/>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>


   <Frame name="GreatVaultListFrame" enableMouse="true" parent="UIParent" mixin="GreatVaultListMixin" inherits="PortraitFrameTemplate, TabSystemOwnerTemplate" >
		<!-- <Size x="800" y="538"/> -->
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Frames>
			<Frame parentKey="Drag"  parent="GreatVaultListFrame" >
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="0" y="0"/>
					<Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="TOPRight" x="0" y="-70"/>
				</Anchors>
			</Frame>
			<Frame parentKey="BrowseResultsFrame" inherits="GreatVaultListBrowseResultsFrameTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="0" y="-70"/>
					<Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="-15" y="0"/>
				</Anchors>
			</Frame>

         <Frame parentKey="OptionsFrame"  parent="GreatVaultListFrame" >
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="0" y="-70"/>
					<Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="-15" y="0"/>
				</Anchors>
			</Frame>
         <Frame parentKey="TabSystem" inherits="TabSystemTemplate">
				<KeyValues>
					<KeyValue key="minTabWidth" value="50" type="number"/>
					<KeyValue key="maxTabWidth" value="150" type="number"/>
					<KeyValue key="tabSelectSound" value="SOUNDKIT.UI_CLASS_TALENT_TAB" type="global"/>
				</KeyValues>
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="BOTTOMLEFT" x="22" y="2"/>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>

   

</Ui>